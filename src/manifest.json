{
  "name": "site-block",
  "description": "Developer build of the site blocker. Not for normal use.",
  "version": "1.0.1",
  "permissions": [
    "storage",
    "tabs",
    "identity",
    "alarms"
  ],
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "key": "",
  "oauth2": {
    "client_id": "92037429831-ln2o8drcoc3kn4m1moa8hsnirus21lkj.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "site-block",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "64": "icons/glasses-red.png"
    }
  },
  "background": {
    "scripts": [
      "background.js",
      "backgroundCalendar.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
