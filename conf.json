{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./src", "./src/popup", "./README.md"],
        "includePattern": "\\.(vue|js)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["node_modules/jsdoc-vuejs"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}